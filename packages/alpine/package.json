{
    "name": "laravel-precognition-alpine",
    "version": "0.5.12",
    "description": "Laravel Precognition (Alpine).",
    "keywords": [
        "laravel",
        "precognition",
        "alpine"
    ],
    "homepage": "https://github.com/laravel/precognition",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/laravel/precognition"
    },
    "license": "MIT",
    "author": "Laravel",
    "main": "dist/index.js",
    "files": [
        "/dist"
    ],
    "scripts": {
        "watch": "rm -rf dist && tsc --watch --preserveWatchOutput",
        "build": "rm -rf dist && tsc",
        "typeCheck": "tsc --noEmit",
        "prepublishOnly": "npm run build",
        "version": "npm pkg set dependencies.laravel-precognition=$npm_package_version"
    },
    "peerDependencies": {
        "alpinejs": "^3.12.1"
    },
    "dependencies": {
        "laravel-precognition": "0.5.12",
        "lodash-es": "^4.17.21"
    },
    "devDependencies": {
        "@types/alpinejs": "^3.7.1",
        "@types/lodash-es": "^4.17.12",
        "typescript": "^5.0.0"
    }
}
